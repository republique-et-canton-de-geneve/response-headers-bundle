{
    "name": "republique-et-canton-de-geneve/response-headers-bundle",
    "version": "1.0.4",
    "description": "A Symfony bundle to easily send headers in your HTTP response",
    "type": "symfony-bundle",
    "require": {
        "php": ">=8.1",
        "symfony/dependency-injection": ">=6.4",
        "symfony/http-kernel": ">=6.4",
        "symfony/config": ">=6.4",
        "symfony/expression-language": ">=6.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.4",
        "ekino/phpstan-banned-code": "^3.0",
        "phpstan/extension-installer": "^1.4",
        "phpstan/phpstan": "^2.1",
        "friendsofphp/php-cs-fixer": "^3.49",
        "rector/rector": "^2.0.11"
    },
    "license": "Apache-2.0",
    "keywords": [
        "bundle",
        "symfony",
        "response",
        "header",
        "headers",
        "http",
        "condtion"
    ],
    "autoload": {
        "psr-4": {
            "EtatGeneve\\ResponseHeadersBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EtatGeneve\\ResponseHeadersBundle\\Tests\\": "Tests/"
        }
    },
    "authors": [
        {
            "name": "Michel Bobillier aka Athos99",
            "email": "athos99@bluewin.ch",
            "role": "Senior Lead Developer"
        }
    ],
    "minimum-stability": "stable",
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
